<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<div class="digg_pagination">
  <div class="page_info">
    <%= page_entries_info @users %>
  </div>
  <%= will_paginate @users %>
</div>

<table class="table" >
  <thead>
    <th>id</th>
    <th>name</th>
    <th>email</th>
    <th>created_at</th>
    <th>updated_at</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= link_to user.name, user_path(user), class: "link hover"%></td>
        <td><%= user.email %></td>
        <td><%= user.created_at.strftime("%F %X") %></td>
        <td><%= user.updated_at.strftime("%F %X") %></td>
      </tr>
    <% end %>
  </tbody>

</table>


<div class="digg_pagination">
  <%= will_paginate @users %>
</div>
